    {
      "functionName": "XSArea",
      "parameters": ["NumRolls", "BeltWidth", "SurAng", "CentLen", "intAng", "intLen", "wingAng", "edgeDist"],
      "formula": [
      "LET(",
      "    theta, 2 * RADIANS(SurAng),",
      "    x_1, CentLen / 2,",
      "    y_1, 0,",
      "    intCosine, COS(RADIANS(intAng)),",
      "    intSine, SIN(RADIANS(intAng)),",
      "    wingCosine, COS(RADIANS(wingAng)),",
      "    wingSine, SIN(RADIANS(wingAng)),",
      "    x_2, x_1 + intLen * intCosine,",
      "    y_2, y_1 + intLen * intSine,",
      "    beltWingLen, BeltWidth / 2 - CentLen / 2 - intLen,",
      "    x_3, x_2 + beltWingLen * wingCosine,",
      "    y_3, y_2 + beltWingLen * wingSine,",
      "    out_1, IFS(",
      "        edgeDist > BeltWidth / 2,",
      "        -1000,",
      "        edgeDi,st < beltWingLen,",
      "        LET(",
      "            x_4, x_2 + (beltWingLen - edgeDist) * wingCosine,",
      "            y_4, y_2 + (beltWingLen - edgeDist) * wingSine,",
      "            A_1, 2 * (x_1 * y_4), // A1",
      "            A_2, 0.5 * ((x_4 - x_1) * (y_4 - y_1)), // A2",
      "            A_3, (x_2 - x_1) * (y_4 - y_2), // A3",
      "            A_4, 0.5 * ((x_4 - x_2) * (y_4 - y_2)), // A4",
      "            VSTACK(A_1, A_2, A_3, A_4, x_4, y_4)",
      "        ),",
      "        edgeDist < beltWingLen + intLen,",
      "        LET(",
      "            x_4, x_2 + (intLen - (edgeDist - beltWingLen)) * intCosine,",
      "            y_4, y_2 + (intLen - (edgeDist - beltWingLen)) * intSine,",
      "            A_1, 2 * (x_1 * y_4), // A1",
      "            A_2, 0.5 * ((x_2 - x_1) * (y_2 - y_1)), // A2",
      "            A_3, 0, // A3",
      "            A_4, 0, // A4",
      "            VSTACK(A_1, A_2, A_3, A_4, x_4, y_4)",
      "        ),",
      "        TRUE,",
      "        LET(",
      "            x_4, x_1 - (edgeDist - beltWingLen - intLen),",
      "            y_4, 0,",
      "            A_1, 0, // A1",
      "            A_2, 0, // A2",
      "            A_3, 0, // A3",
      "            A_4, 0, // A4",
      "            VSTACK(A_1, A_2, A_3, A_4, x_4, y_4)",
      "        )",
      "    ),",
      "    A_1, INDEX(out_1, 1),",
      "    A_2, INDEX(out_1, 2),",
      "    A_3, INDEX(out_1, 3),",
      "    A_4, INDEX(out_1, 4),",
      "    x_4, INDEX(out_1, 5),",
      "    y_4, INDEX(out_1, 6),",
      "    radius, x_4 / SIN(RADIANS(SurAng)),",
      "    A_5, radius ^ 2 / 2 * (theta - SIN(theta)),",
      "    VSTACK(",
      "        HSTACK(\"Total Area\", (A_1 + 2 * (A_2 + A_3 + A_4) + A_5) / 1000 ^ 2),",
      "        HSTACK(\"A1\", A_1),",
      "        HSTACK(\"A2\", A_2),",
      "        HSTACK(\"A3\", A_3),",
      "        HSTACK(\"A4\", A_4),",
      "        HSTACK(\"A5\", A_5),",
      "        HSTACK(\"X1\", x_1),",
      "        HSTACK(\"Y1\", y_1),",
      "        HSTACK(\"X2\", x_2),",
      "        HSTACK(\"Y2\", y_2),",
      "        HSTACK(\"X3\", x_3),",
      "        HSTACK(\"Y3\", y_3),",
      "        HSTACK(\"X4\", x_4),",
      "        HSTACK(\"Y4\", y_4)",
      "        )",
      "    )",
      ")"
  ],
      "description": "From P.W./A.A design sheets, NC"
    }
  ]
